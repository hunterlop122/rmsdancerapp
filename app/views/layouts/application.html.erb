<!DOCTYPE html>
<html>
  <head>
    <title>Rmsdancerapp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <div id="navigation">
    <div id="navigation_logo">
      <%= image_tag("rmslogo.png") %>
    </div>
    <div id="navigation_text">
       <%= link_to 'New', new_dancer_url %> <%= link_to 'Show', "/dancers/show"  %> <%= link_to 'LogOut', destroy_user_session_path, method: :delete %>
    </div>
  </div>
    <p class="notice"><%= notice %></p>
       <p class="alert"><%= alert %></p>
    <%= yield %>
  </body>
</html>
